      <!-- Microsoft's Northwind data regions accessed by the perf_test app. -->
   <region name="nw">
      <region name="categories" refid="PARTITION"></region>
      <region name="customers" refid="PARTITION">
         <region-attributes gateway-sender-ids="ny-to-ln-customers" refid="PARTITION"/>
      	<index name="pk">
      		<primary-key field="customerId" />
      	</index>
      	<index name="customerId">
            <functional from-clause="/nw/customers c" expression="c.customerId" />
         </index>
      </region>
      <region name="employees" refid="PARTITION">
      	<index name="pk">
      		<primary-key field="employeeId" />
      	</index>
      	<index name="employeeId">
      		<functional from-clause="/nw/employees e" expression="e.employeeId"/>
      	</index>
      </region>
      <region name="employee_territories" refid="PARTITION"></region>
      <region name="orders" refid="PARTITION">
         <region-attributes refid="customerIdentityKey" gateway-sender-ids="ny-to-ln-customers" />
      	<index name="pk">
      		<primary-key field="orderId" />
      	</index>
      	<index name="orderId">
      		<functional from-clause="/nw/orders o" expression="o.orderId"/>
      	</index>
      	<index name="customerId">
      		<functional from-clause="/nw/orders o" expression="o.customerId"/>
      	</index>
      	<index name="employeeId">
      		<functional from-clause="/nw/orders o" expression="o.employeeId"/>
      	</index>
      </region>
      <region name="order_details" refid="PARTITION"></region>
      <region name="products" refid="PARTITION"></region>
      <region name="regions" refid="PARTITION"></region>
      <region name="shippers" refid="PARTITION"></region>
      <region name="suppliers" refid="PARTITION"></region>
      <region name="territories" refid="PARTITION"></region>
   </region>

    <region name="lru_entry_overflow">
    	<region-attributes refid="PARTITION">
    		<eviction-attributes><lru-entry-count action="overflow-to-disk" maximum="100"/></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="lru_entry_destroy">
    	<region-attributes refid="PARTITION">
    		<eviction-attributes><lru-entry-count action="local-destroy" maximum="100"/></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="lru_heap_destroy">
    	<region-attributes refid="PARTITION">
    		<eviction-attributes><lru-heap-percentage action="local-destroy"></lru-heap-percentage></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="lru_memory_destroy">
   	<region-attributes refid="PARTITION">
    		<!-- maximum in MB -->
    		<eviction-attributes><lru-memory-size action="local-destroy" maximum="20"></lru-memory-size></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="expiration">
    	<region-attributes refid="PARTITION">
    		<!-- timeout in seconds -->
    		<entry-time-to-live><expiration-attributes timeout="60"></expiration-attributes></entry-time-to-live>
    		<entry-idle-time><expiration-attributes timeout="30"></expiration-attributes></entry-idle-time>
    	</region-attributes>
   </region>

   <!-- Replicated Regions (rr) -->
    <region name="rr_lru_entry_overflow">
    	<region-attributes refid="REPLICATE">
    		<eviction-attributes><lru-entry-count action="overflow-to-disk" maximum="100"/></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="rr_lru_entry_destroy">
    	<region-attributes refid="REPLICATE">
         <eviction-attributes><lru-entry-count action="local-destroy" maximum="100"/></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="rr_lru_heap_destroy">
    	<region-attributes refid="REPLICATE">
    		<eviction-attributes><lru-heap-percentage action="local-destroy"></lru-heap-percentage></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="rr_lru_memory_destroy">
   	<region-attributes refid="REPLICATE">
    		<!-- maximum in MB -->
    		<eviction-attributes><lru-memory-size action="local-destroy" maximum="20"></lru-memory-size></eviction-attributes>
    	</region-attributes>
    </region>
   <region name="rr_expiration">
    	<region-attributes refid="REPLICATE">
    		<!-- timeout in seconds -->
    		<entry-time-to-live><expiration-attributes timeout="60"></expiration-attributes></entry-time-to-live>
    		<entry-idle-time><expiration-attributes timeout="30"></expiration-attributes></entry-idle-time>
    	</region-attributes>
   </region>

   <!-- off-heap regions -->
   <region name="off_heap_lru_entry_overflow">
    	<region-attributes refid="PARTITION" off-heap="true">
    		<eviction-attributes><lru-entry-count action="overflow-to-disk" maximum="100"/></eviction-attributes>
    	</region-attributes>
   </region>
   <region name="off_heap_lru_entry_destroy">
    	<region-attributes refid="PARTITION" off-heap="true">
    		<eviction-attributes><lru-entry-count action="local-destroy" maximum="100"/></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="off_heap_lru_heap_destroy">
    	<region-attributes refid="PARTITION" off-heap="true">
    		<eviction-attributes><lru-heap-percentage action="local-destroy"></lru-heap-percentage></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="off_heap_lru_memory_destroy">
   	<region-attributes refid="PARTITION" off-heap="true">
    		<!-- maximum in MB -->
    		<eviction-attributes><lru-memory-size action="local-destroy" maximum="20"></lru-memory-size></eviction-attributes>
    	</region-attributes>
    </region>
    <region name="off_heap_expiration">
       <region-attributes refid="PARTITION" off-heap="true">
    		<!-- timeout in seconds -->
    		<entry-time-to-live><expiration-attributes timeout="60"></expiration-attributes></entry-time-to-live>
    		<entry-idle-time><expiration-attributes timeout="30"></expiration-attributes></entry-idle-time>
    	</region-attributes>
   </region>

   <function-service>
      <function>
         <class-name>com.mastercard.gemfire.function.McStatsFunction</class-name>
      </function>
      <function>
         <class-name>com.mastercard.gemfire.function.McStatsSamplerFunction</class-name>
      </function>
      <function>
         <class-name>com.mastercard.gemfire.function.McJmxFunction</class-name>
      </function>
   </function-service>

   <resource-manager critical-heap-percentage="99" eviction-heap-percentage="96"
      critical-off-heap-percentage="99.9" eviction-off-heap-percentage="85"
      />

     <!-- StatsInitialzer initializes custom stats. By default, custom
      JMX beans are enabled. To diusable it, set "mc.jmx.enable" to false.
      Must match "true", otherwise, false. Case-insensitive. -->
   <initializer>
      <class-name>com.mastercard.gemfire.McStatsInitializer</class-name>
      <parameter name="mc.jmx.enabled">
         <string>true</string>
      </parameter>
      <!-- initialDelay in seconds -->
      <parameter name="McStatsSampler.initialDelay">
         <string>0</string>
      </parameter>
      <!-- period in seconds -->
      <parameter name="McStatsSampler.period">
         <string>10</string>
      </parameter>
   </initializer>

</cache>
