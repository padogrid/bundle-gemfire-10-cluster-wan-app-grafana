                       dispatcher-threads="5" enable-batch-conflation="false" batch-size="100000" batch-time-interval="10000">
    </async-event-queue>
    <async-event-queue id="VelUpdateKCOutputQueue" disk-store-name="VelocityUpdateBatchStore" persistent="true" parallel="true"
                       dispatcher-threads="5" enable-batch-conflation="false" batch-size="100000" batch-time-interval="10000">
    </async-event-queue>
    <async-event-queue id="VelUpdateOutputQueue" disk-store-name="VelocityUpdateBatchStore" persistent="true" parallel="true"
                       dispatcher-threads="5" enable-batch-conflation="false" batch-size="100000" batch-time-interval="10000">
    </async-event-queue>
            -->

    <cache-server port="${geode-addon.server.port}" max-connections="50000"><group>${SERVER_GROUP}</group></cache-server>

