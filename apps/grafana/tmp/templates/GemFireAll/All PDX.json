                "instance": 1,
                "job": 0,
                "job 2": 4,
                "job 3": 5,
                "job 4": 6,
                "tag0": 2,
                "tag0 2": 7,
                "tag0 3": 8,
                "tag0 4": 9
              },
              "renameByName": {
                "Value #A": "Creation Time",
                "Value #Access Time": "Access Time",
                "Value #C": "",
                "Value #Creation Time": "Creation Time",
                "Value #Decrement Count": "Decrements",
                "Value #Increment Count": "Increments",
                "Value #Update Time": "Update Time",
                "Value #Value": "Value",
                "instance": "Member",
                "instance 1": "Member",
                "job": "Cluster",
                "tag0": "PN Counter",
                "tag0 1": "PNCounter"
              }
            }
          },
          {
            "id": "sortBy",
            "options": {
              "fields": {},
              "sort": [
