        "description": "PN Counter creation time and operation count",
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "cellOptions": {
                "type": "auto"
              },
              "inspect": false
            },
            "mappings": [
              {
                "options": {
                  "pattern": "^.*(=)(\\w.*).*\"$",
                  "result": {
                    "index": 0,
                    "text": "$2"
                  }
                },
                "type": "regex"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "locale"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/.*Time/"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "dateTimeAsIsoNoDateIfToday"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Member"
              },
              "properties": [
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "Member Link",
                      "url": "/d/c5932a49-ac31-44d9-95fe-b3eeb7ac974f/member?orgId=1&refresh=5s&var-DataSource=$DataSource&var-Cluster=${__data.fields.Cluster}&var-Member=${__value.raw}&${__url_time_range}"
                    }
